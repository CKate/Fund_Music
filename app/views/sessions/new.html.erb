<div class= "container
	<div class="panel-group">
		<div class="panel panel-default">
			<div class = "panel-heading">
				<h1>Log in</h1>
			</div>
			<div class = "panel-body">
				<%= form_tag do %>
					<%= label_tag :email%>
					<%= text_field_tag :email%>
					<%= label_tag :password %>
					<%= password_field_tag :password %>
					<%= submit_tag "Log in"%>
				<% end %>
			</div>
		</div>
	</div>
</div>