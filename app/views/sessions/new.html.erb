<div class= "container">
	<div class="panel-group formPanel">
		<div class="panel panel-default">
			<div class = "panel-heading">
				<h1 class="header">Log in</h1>
			</div>
			<div class = "panel-body">
				<%= form_tag({:action=>'create'}, {:class=>'form-horizontal'})do %>
					<div class = "form-group">
						<%= label_tag :email%>
						<%= text_field_tag :email, "example@gmail.com", :class=>"form-control" %>
					</div>
					<div class = "form-group">
						<%= label_tag :password %>
						<%= password_field_tag :password, "password", :class=>"form-control"%>
					</div>
					<%= submit_tag "Log in", :class=>"btn btn-default"%>
				<% end %>
			</div>
		</div>
	</div>
</div>